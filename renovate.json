{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "regexManagers": [
    {
      "fileMatch": [
        "^\\.github/workflows/docker-publish\\.yml$"
      ],
      "matchStrings": [
        "cosign-release:\\s*'(?<currentValue>v[0-9]+\\.[0-9]+\\.[0-9]+)'"
      ],
      "datasourceTemplate": "github-releases",
      "depNameTemplate": "sigstore/cosign"
    }
  ]
}
